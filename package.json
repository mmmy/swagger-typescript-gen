{
  "name": "swagger-typescript-gen",
  "main": "./lib/codegen.js",
  "version": "0.1.0",
  "description": "A Swagger Codegenerator tailored for typescript.",
  "scripts": {
    "pretest": "npm run build",
    "test": "echo 'no test!'",
    "clean": "rm -rf tmp-*",
    "build": "tsc --project tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "bin": {
    "swagger2ts": "bin/swagger2ts.js"
  },
  "bugs": {
    "url": "https://github.com/mmmy/swagger-typescript-gen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mmmmy/swagger-typescript-gen.git"
  },
  "keywords": [
    "swagger",
    "rest"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/mmmy/swagger-typescript-gen",
  "dependencies": {
    "commander": "^2.20.3",
    "js-beautify": "^1.13.0",
    "jshint": "^2.12.0",
    "lodash": "^4.17.20",
    "mustache": "^3.2.1",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.14",
    "@types/js-beautify": "^1.11.0",
    "@types/lodash": "^4.14.161",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10.17.35",
    "final-fs": "^1.6.0",
    "grunt": "^1.3.0",
    "grunt-contrib-jshint": "^2.1.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-vows": "^0.4.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.2.1",
    "matchdep": "^2.0.0",
    "prettier": "1.15.3",
    "request": "^2.88.2",
    "superagent": "^4.1.0",
    "tmp": "0.0.33",
    "typescript": "^3.9.7",
    "vows": "^0.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md},": [
      "prettier --write",
      "git add"
    ]
  }
}
